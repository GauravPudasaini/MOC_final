digraph {
	rankdir=LR
	node [shape=point]
	qi
	node [color=black shape=circle style=""]
	q0
	qi -> q0 [label=start]
	node [color=black shape=circle style=""]
	q1
	node [color=black shape=circle style=""]
	q2
	node [color=green shape=doublecircle style=""]
	q3
	q0 -> q0 [label=a]
	q0 -> q1 [label=b]
	q1 -> q1 [label=a]
	q1 -> q2 [label=b]
	q2 -> q2 [label=a]
	q2 -> q3 [label=b]
	q3 -> q3 [label="a+b"]
label = "\n\nDFA Graph"
}
